# Print numbers 1 to 10 with their squares
for i in range(1, 11):
    print(f"Number: {i}, Square: {i*i}")

# Calculate sum of numbers 1 to 10
total = sum(range(1, 11))
print("\nSum of numbers 1 to 10:", total)

